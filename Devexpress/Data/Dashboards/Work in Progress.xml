        <CalculatedField Name="hoursVariancePercent" Expression="Iif(SUM([currentbudgethours])=0,0 ,SUM(([hoursVariance]))/SUM([currentbudgethours]) )" DataType="Auto" DataMember="profitlosssummarybyprojectwithbudget" />
