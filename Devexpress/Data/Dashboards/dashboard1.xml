<?xml version="1.0" encoding="utf-8"?>
<Dashboard>
  <Title Text="mssqlDashboard" />
  <DataSources>
    <SqlDataSource Name="coa" ComponentName="sqlDataSource1">
      <Connection Name="MssqlConnectionString" FromAppConfig="true" />
      <Query Type="SelectQuery" Name="coa">
        <Tables>
          <Table Name="coa" />
        </Tables>
        <Columns>
          <Column Table="coa" Name="Id" />
          <Column Table="coa" Name="description" />
          <Column Table="coa" Name="accountType" />
          <Column Table="coa" Name="statementType" />
          <Column Table="coa" Name="jobCostCategory" />
          <Column Table="coa" Name="accountCategory" />
        </Columns>
      </Query>
      <ConnectionOptions CloseConnection="true" />
    </SqlDataSource>
  </DataSources>
  <Items>
    <Grid ComponentName="gridDashboardItem1" Name="Grid 1" DataSource="sqlDataSource1" DataMember="coa">
      <DataItems>
        <Measure DataMember="Id" DefaultId="DataItem0" />
        <Measure DataMember="accountCategory" DefaultId="DataItem1" />
        <Dimension DataMember="accountType" DefaultId="DataItem2" />
      </DataItems>
      <GridColumns>
        <GridMeasureColumn>
          <Measure DefaultId="DataItem0" />
        </GridMeasureColumn>
        <GridMeasureColumn>
          <Measure DefaultId="DataItem1" />
        </GridMeasureColumn>
        <GridDimensionColumn>
          <Dimension DefaultId="DataItem2" />
        </GridDimensionColumn>
      </GridColumns>
      <GridOptions />
    </Grid>
    <Pie ComponentName="pieDashboardItem1" Name="Pies 1" DataSource="sqlDataSource1" DataMember="coa">
      <DataItems>
        <Measure DataMember="Id" DefaultId="DataItem0" />
        <Measure DataMember="accountCategory" DefaultId="DataItem2" />
        <Measure DataMember="accountType" SummaryType="Count" DefaultId="DataItem1" />
      </DataItems>
      <Values>
        <Value DefaultId="DataItem0" />
        <Value DefaultId="DataItem2" />
        <Value DefaultId="DataItem1" />
      </Values>
    </Pie>
  </Items>
  <LayoutTree>
    <LayoutGroup>
      <LayoutGroup Orientation="Vertical">
        <LayoutItem DashboardItem="gridDashboardItem1" />
        <LayoutItem DashboardItem="pieDashboardItem1" />
      </LayoutGroup>
    </LayoutGroup>
  </LayoutTree>
</Dashboard>